<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks Condition="$([MSBuild]::IsOSPlatform('windows'))">net462;net471;net8.0;net9.0</TargetFrameworks>
    <TargetFrameworks Condition="!$([MSBuild]::IsOSPlatform('windows'))">net8.0;net9.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <Platforms>AnyCPU;x64</Platforms>
    <ApplicationIcon>Sample.ico</ApplicationIcon>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <AssemblyName>DebuggerIntegration.Python.AlternetUI</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Content\**" />
    <Compile Remove="Resources\**" />
    <EmbeddedResource Remove="Content\**" />
    <EmbeddedResource Remove="Resources\**" />
    <None Remove="Content\**" />
    <None Remove="Resources\**" />
    <UIXml Remove="Content\**" />
    <UIXml Remove="Resources\**" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Sample.ico" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Sample.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Formats.Asn1" Version="6.0.1" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Alternet.Studio.Common.AlternetUI" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Common" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Common.Python" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Editor.AlternetUI" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Editor.Common.AlternetUI" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Scripter.Integration.AlternetUI" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Syntax" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Syntax.Parsers.Advanced.Python" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Scripter.Python" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Scripter.Debugger.Python" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Scripter.Python.Embedded" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Scripter.Common" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Scripter.Debugger.Common" Version="10.0.1" />
    <PackageReference Include="Alternet.Studio.Scripter.Debugger.UI.AlternetUI" Version="10.0.1" />
    <PackageReference Include="Alternet.UI" Version="0.9.736-beta" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties/" />
  </ItemGroup>
</Project>