<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>Threading.Python</AssemblyName>
    <RootNamespace>Threading</RootNamespace>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
  </PropertyGroup>
  <PropertyGroup Condition="'$(FastBuildTargets)'!=''">
    <TargetFrameworks>$(FastBuildTargets)</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(FastBuildTargets)'==''">
    <TargetFrameworks>net462;net471;net8.0-windows;net9.0-windows</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resources\Icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="MainForm.cs" />
    <Compile Include="MainForm.Designer.cs" />
    <Compile Include="Program.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="MainForm.resx" />
    <EmbeddedResource Condition="'$(TargetFrameworkIdentifier)' == '.NETFramework'" Include="Properties\licenses.licx" />
    <EmbeddedResource Include="Resources\Icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Alternet.Studio.Common" Version="10.0.7" />
    <PackageReference Include="Alternet.Studio.Common.Python" Version="10.0.7" />
    <PackageReference Include="Alternet.Studio.Editor.WindowsForms" Version="10.0.7" />
    <PackageReference Include="Alternet.Studio.Editor.Common" Version="10.0.7" />
    <PackageReference Include="Alternet.Studio.Scripter.Python" Version="10.0.7" />
    <PackageReference Include="Alternet.Studio.Scripter.Python.Embedded" Version="10.0.7" />
    <PackageReference Include="Alternet.Studio.Scripter.Common" Version="10.0.7" />
  </ItemGroup>
  <ItemGroup />
</Project>