<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>ExpressionEvaluation.IronPython</AssemblyName>
    <RootNamespace>ExpressionEvaluation</RootNamespace>
    <EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
  </PropertyGroup>
  <PropertyGroup Condition="'$(FastBuildTargets)'!=''">
    <TargetFrameworks>$(FastBuildTargets)</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(FastBuildTargets)'==''">
    <TargetFrameworks>net462;net6.0-windows;net7.0-windows;net8.0-windows</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <OutputPath>..\..\..\..\bin\Release\Demos.Scripter.IronPython\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="MainForm.cs" />
    <Compile Include="MainForm.Designer.cs" />
    <Compile Include="Program.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="MainForm.resx" />
    <EmbeddedResource Condition="$(DefineConstants.Contains(';NETFRAMEWORK'))" Include="Properties\licenses.licx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Alternet.Studio.Common" Version="9.5.2" />
    <PackageReference Include="Alternet.Studio.Common.Python" Version="9.5.2" />
    <PackageReference Include="Alternet.Studio.Editor.WindowsForms" Version="9.5.2" />
    <PackageReference Include="Alternet.Studio.Editor.Common" Version="9.5.2" />
    <PackageReference Include="Alternet.Studio.Scripter.IronPython" Version="9.5.2" />
    <PackageReference Include="Alternet.Studio.Scripter.Common" Version="9.5.2" />
  </ItemGroup>
  <ItemGroup />
</Project>