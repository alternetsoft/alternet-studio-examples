<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>CustomAssembly.IronPython</AssemblyName>
    <RootNamespace>CustomAssembly</RootNamespace>
    <EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
  </PropertyGroup>
  <PropertyGroup Condition="'$(FastBuildTargets)'!=''">
    <TargetFrameworks>$(FastBuildTargets)</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(FastBuildTargets)'==''">
    <TargetFrameworks>net462;net471;net8.0-windows;net9.0-windows</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="ExternalAssembly\**" />
    <EmbeddedResource Remove="ExternalAssembly\**" />
    <None Remove="ExternalAssembly\**" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Resources\Icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="MainForm.cs" />
    <Compile Include="MainForm.Designer.cs" />
    <Compile Include="Program.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="MainForm.resx" />
    <EmbeddedResource Condition="$(DefineConstants.Contains(';NETFRAMEWORK'))" Include="Properties\licenses.licx" />
    <EmbeddedResource Include="Resources\Icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Alternet.Studio.Common" Version="10.0.3" />
    <PackageReference Include="Alternet.Studio.Common.Python" Version="10.0.3" />
    <PackageReference Include="Alternet.Studio.Editor.WindowsForms" Version="10.0.3" />
    <PackageReference Include="Alternet.Studio.Editor.Common" Version="10.0.3" />
    <PackageReference Include="Alternet.Studio.Syntax.Parsers.Advanced" Version="10.0.3" />
    <PackageReference Include="Alternet.Studio.Syntax" Version="10.0.3" />
    <PackageReference Include="Alternet.Studio.Syntax.Parsers.Advanced.Python" Version="10.0.3" />
    <PackageReference Include="Alternet.Studio.Scripter.IronPython" Version="10.0.3" />
    <PackageReference Include="Alternet.Studio.Scripter.Common" Version="10.0.3" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="ExternalAssembly">
      <HintPath>ExternalAssembly.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <None Update="ExternalAssembly.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="ExternalAssembly.pdb">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup />
</Project>